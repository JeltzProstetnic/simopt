<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:DiagramDesigner="clr-namespace:MatthiasToolbox.GraphDesigner.DiagramDesigner">
    <DiagramDesigner:Toolbox 
        x:Key="AutomationStencils" 
        ItemSize="55,45">
        <ItemsControl.Items>
            <Grid
                IsHitTestVisible="True"
                Margin="2"
                ToolTip="Verzögerung" 
                MinHeight="30" MinWidth="40">
                <Image  Stretch="Fill" Source="pack://application:,,,/MatthiasToolbox.GraphDesigner;component/Images/rt1.png"/>
                <Rectangle Stroke="Black" StrokeThickness="1"/>
            </Grid>
            <Grid
                IsHitTestVisible="True" 
                Margin="2"
                ToolTip="PT2" 
                MinHeight="30" MinWidth="40">
                <Image  Stretch="Fill" Source="pack://application:,,,/MatthiasToolbox.GraphDesigner;component/Images/rt2.png"/>
                <Rectangle Stroke="Black" StrokeThickness="1"/>
            </Grid>
            <Grid
                IsHitTestVisible="True" 
                Margin="2"
                ToolTip="PI" 
                MinHeight="30" MinWidth="40">
                <Image  Stretch="Fill" Source="pack://application:,,,/MatthiasToolbox.GraphDesigner;component/Images/rt3.png"/>
                <Rectangle Stroke="Black" StrokeThickness="1"/>
            </Grid>
            <Grid
                IsHitTestVisible="True" 
                Margin="2"
                ToolTip="Kennlinie" 
                MinHeight="30" MinWidth="40">
                <Image  Stretch="Fill" Source="pack://application:,,,/MatthiasToolbox.GraphDesigner;component/Images/rt4.png"/>
                <Rectangle Stroke="Black" StrokeThickness="1"/>
            </Grid>
            <Grid 
                IsHitTestVisible="True" 
                Margin="2"
                ToolTip="Cosinus" 
                MinHeight="30" MinWidth="40">
                <Image  Stretch="Fill" Source="pack://application:,,,/MatthiasToolbox.GraphDesigner;component/Images/rt5.png"/>
                <Rectangle Stroke="Black" StrokeThickness="1"/>
            </Grid>
            <Grid 
                IsHitTestVisible="True"
                Margin="2"
                ToolTip="Zweipunkt" 
                MinHeight="30" MinWidth="40">
                <Image  Stretch="Fill" Source="pack://application:,,,/MatthiasToolbox.GraphDesigner;component/Images/rt6.png"/>
                <Rectangle Stroke="Black" StrokeThickness="1"/>
            </Grid>
        </ItemsControl.Items>
    </DiagramDesigner:Toolbox>
</ResourceDictionary>